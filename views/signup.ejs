  <body class="text-center">

<main class="form-signin w-100 m-auto">
  <form action="/" method="post" class="needs-validation" id="sign-up-form" novalidate>

    <div class="box" id="heading">
        <h1>Sign-Up </h1>
    </div>
    <div class="form-floating">
      <input id="input1" type="text" name="username" class="form-control top" placeholder="username" required>
      <label for="floatingInput">username</label>
      <div class="invalid-feedback">Please enter a value.</div>
    </div>
    <div class="form-floating">
      <input id="input2" type="password" name="password" class="form-control middle" placeholder="Password" required>
      <label for="floatingPassword">password</label>
      <div class="invalid-feedback">Please enter a value.</div>
    </div>
    <div class="form-floating">
      <input id="input3" type="email" name="email" class="form-control bottom" placeholder="email" required>
      <label for="floatingPassword">email</label>
      <div class="invalid-feedback">Please enter a value.</div>
    </div>
    <div class="form-floating">
        <button class="w-100 btn btn-lg todo" type="submit">Sign up</button>
    </div>
    <div class="form-floating">
        <p>Already an user? <a href="/login">click here</a></p>
    </div>
  </form>
</main>
    <script src="scripts/validator.js"></script> 
    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.js"></script>
    <% if(error) { %>
        <script>
          $(document).ready(function(){
            alert("<%= errorMsg %>");
          });
        </script>
    <% } %>
    <script src="js/bootstrap.bundle.min.js"></script>
    <script src="js/theme.js"></script>
    

  </body>
</html>
