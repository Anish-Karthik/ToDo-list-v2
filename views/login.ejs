  <body class="text-center">



<main class="form-signin w-100 m-auto">
  <form action="/login" method="post">
    <!-- <img class="mb-4" src="images/logo.png" alt="" width="72" height="57"> -->

    <div class="box" id="heading">
        <h1>Login</h1>
    </div>
    <div class="form-floating">
      <input type="text" name="username" class="form-control top"  placeholder="username">
      <label for="floatingInput">username</label>
    </div>
    <div class="form-floating">
      <input type="password" name="password" class="form-control bottom" placeholder="Password">
      <label for="floatingPassword">password</label>
    </div>
    <div class="checkbox mb-3">
      <label >
        <input type="checkbox" value="remember-me" > Remember me
      </label>
    </div>
    <button class="w-100 btn btn-lg todo" type="submit" style="margin-top: 0;">Login</button>
    <!-- <p class="mt-5 mb-3 text-body-secondary">&copy; Anish Karthik</p> -->
    <br><br>
    <div class="form-floating">
      <p>dont't have an account? <a href="/">click here</a></p>
    </div>
  </form>

</main>
    <script src="script/validator.js"></script> 
    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.js"></script>
    <% if(error) { %>
        <script>
          $(document).ready(function(){
            alert("<%= errorMsg %>");
          });
        </script>
    <% } %>
